$computers=get-adcomputer-filter{Name -like "USCARITLW6DVXY"} | select-object -expandproperty Name foreach ($computer in $computers) { $serial = Get-WMIObject -Class Win32_BIOS -ComputerName $computer | select-object -expandproperty SerialNumber write-output ("Computer {0} Serial Number: {1}" -f $computer,$serial) }
USCARITLW6DVXY


Function Get-Lastused{
<#
  
.EXAMPLE
        PS C:\> Get-Lastused
       
                        
.EXAMPLE
        PS C:\> Get-Lastused -Computer server1,server2,server3
       
************************************************************************
 Notes: Run as administrator
 Author: Jiten https://community.spiceworks.com/people/jitensh
Date Created: 1/21/2019
     Credits: 
Last Revised: 
************************************************************************
#>
[cmdletbinding()]
   param(
   [String[]]$Computer = $env:COMPUTERNAME
   )
Foreach($Computer in $Computer){
If(Test-Connection -ComputerName $computer -BufferSize 16 -Count 1 -ea 0 -quiet)
{
Try
{
$ErrorActionPreference ="stop"
 Get-WmiObject -Class Win32_UserProfile -ComputerName $computer -Filter "Special = $false"|
 select @{n='Computer';e={$computer}}, @{n='User';e={$_.localpath.split('\')[2]}},@{n='lastused';e={$_.ConvertToDateTime($_.LastUseTIme)}},@{n='LoggedInNow';e={(Get-WmiObject win32_ComputerSystem  -ComputerName $computer).username.split('\')[1]}}|Sort-Object lastused -Descending
}
Catch
{
        Write-Host "$computer `b" -NoNewline -BackgroundColor red
        Write-Warning $error[0]
}
}
Else
{
        Write-Host "$($computer) is offline" -ForegroundColor Red
}
}
}
$computers=get-adcomputer -filter {operatingsystem -like '*server*'}|select -exp Name 
                           ForEach($computer in $computers)
                           {
                           Get-Lastused -computer $computer  
                          } 

